c mod = "c.arl"

aon i32[]? mut = c.malloc(sizeof(i32)*10)
if aon == null {
    c.printf("c malloc is failed.")
    c.exit(1)
}
defer c.free(aon)

print_aon_cap () : c.printf("aon.cap is %zu\n", aon.cap)
print_aon_qty () : c.printf("aon.qty is %zu\n", aon.qty)

print_aon_cap() # 10
print_aon_qty() # 0

aon[0] = 10
print_aon_qty() # 1

aon[1] = 23
print_aon_qty() # 2

c.printf("%s", (str)aon) # [10, 23]

c.exit(0)   